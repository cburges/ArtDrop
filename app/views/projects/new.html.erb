<h2>New Project:</h2>
<%= simple_form_for([@user,@project]) do |f| %>
<p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :description %><br>
  <%= f.text_area :description %>
</p>

<p>
  <%= f.label :medium %><br>
  <%= f.text_field :medium %>
</p>

<p>
  <%= f.file_field :images, multiple: true %>
</p>

<p>
  <%= f.submit %>
</p>
<% end %>
